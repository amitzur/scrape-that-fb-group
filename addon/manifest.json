{
  "name": "Facebook Harvester",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Scrape that FB group",
  "homepage_url": "http://www.github.com/amitzur/scrape-that-fb-group",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/groups/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}